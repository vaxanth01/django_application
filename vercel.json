{
  "version": 2,
  "builds": [
    {
      "src": "vasanth_portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python@3.11"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vasanth_portfolio/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "your_project.settings",
    "DATABASE_URL": "@mysql://root@localhost:3306/mmv"
  }
}
